{
  "name": "keycloak-connect",
  "version": "13.0.0-dev",
  "description": "Keycloak Connect Middleware",
  "homepage": "http://keycloak.org",
  "main": "keycloak.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint *.js *.ts example/**/*.js middleware/**/*.js stores/**/*.js test/**/*.js",
    "doc": "./node_modules/.bin/rimraf docs && ./node_modules/.bin/jsdoc -c .jsdoc.json",
    "clean": "./node_modules/.bin/rimraf docs .nyc_output",
    "test_1": "node test/grant-manager-spec.js",
    "test_2": "./node_modules/.bin/tap --no-coverage test/keycloak-connect-web-enforcer-spec.js",
    "test_3": "./node_modules/.bin/tap --no-coverage test/keycloak-connect-rest-mixed-client-spec.js",
    "test_4": "./node_modules/.bin/tap --no-coverage test/keycloak-connect-rest-enforcer-spec.js",
    "test_5": "./node_modules/.bin/tap --no-coverage test/grant-manager-token-timeout-spec.js",
    "test_6": "./node_modules/.bin/tap --no-coverage test/keycloak-connect-rest-spec.js",
    "test_7": "./node_modules/.bin/tap --no-coverage test/keycloak-connect-web-spec.js",
    "test_8": "./node_modules/.bin/tap --no-coverage test/unit/hconfig-test-spec.js",
    "test_9": "./node_modules/.bin/tap --no-coverage test/unit/keycloak-object-test-spec.js",
    "test": "./node_modules/.bin/rimraf .nyc_output && ./node_modules/.bin/nyc ./node_modules/.bin/tap --timeout=60 -j1 test/*-spec.js"
  },
  "keywords": [
    "sso",
    "keycloak",
    "oauth",
    "oauth2.0",
    "authentication"
  ],
  "files": [
    "example",
    "middleware",
    "stores",
    "test",
    "index.js",
    "uuid.js",
    "keycloak.d.ts"
  ],
  "typings": "keycloak.d.ts",
  "author": {
    "name": "Red Hat, Inc.",
    "url": "http://keycloak.org"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "jwk-to-pem": "^2.0.5",
    "keycloak-admin-client": "bucharest-gold/keycloak-admin-client"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "coveralls": "^3.1.0",
    "defaults-deep": "^0.2.4",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "hogan-express": "^0.5.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "keycloak-request-token": "^0.1.0",
    "minimist": "^1.2.5",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "rsa-compat": "^2.0.8",
    "selenium-webdriver": "^3.6.0",
    "server-destroy": "^1.0.1",
    "tap": "^15.0.9",
    "typescript": "^4.2.4"
  },
  "optionalDependencies": {
    "chromedriver": "latest"
  },
  "semistandard": {
    "ignore": [
      "**/keycloak*/"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keycloak/keycloak-nodejs-connect.git"
  },
  "bugs": "https://github.com/keycloak/keycloak-nodejs-connect/issues"
}
