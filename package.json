{
  "name": "keycloak-connect",
  "version": "13.0.0-dev",
  "description": "Keycloak Connect Middleware",
  "homepage": "http://keycloak.org",
  "main": "keycloak.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint *.js *.ts example/**/*.js middleware/**/*.js stores/**/*.js test/**/*.js",
    "doc": "./node_modules/.bin/rimraf docs && ./node_modules/.bin/jsdoc -c .jsdoc.json",
    "clean": "./node_modules/.bin/rimraf docs .nyc_output",
    "test": "./node_modules/.bin/rimraf .nyc_output && ./node_modules/.bin/nyc ./node_modules/.bin/tape test/*-spec.js | tap-nyc"
  },
  "keywords": [
    "sso",
    "keycloak",
    "oauth",
    "oauth2.0",
    "authentication"
  ],
  "files": [
    "example",
    "middleware",
    "stores",
    "test",
    "index.js",
    "uuid.js",
    "keycloak.d.ts",
    "run-tests.sh"
  ],
  "typings": "keycloak.d.ts",
  "author": {
    "name": "Red Hat, Inc.",
    "url": "http://keycloak.org"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "jwk-to-pem": "^2.0.5",
    "keycloak-admin": "^1.14.15"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "blue-tape": "^1.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "coveralls": "^3.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "hogan-express": "^0.5.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "jshint": "^2.12.0",
    "minimist": "^1.2.5",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "rsa-compat": "^2.0.8",
    "selenium-webdriver": "^3.6.0",
    "semistandard": "^16.0.0",
    "server-destroy": "^1.0.1",
    "tap-nyc": "^1.0.3",
    "tape": "^5.2.2",
    "tape-catch": "^1.0.6"
  },
  "optionalDependencies": {
    "chromedriver": "latest"
  },
  "semistandard": {
    "ignore": [
      "**/keycloak*/"
    ]
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keycloak/keycloak-nodejs-connect.git"
  },
  "bugs": "https://github.com/keycloak/keycloak-nodejs-connect/issues"
}
